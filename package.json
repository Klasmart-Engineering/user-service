{
  "name": "kidsloop-user-service",
  "version": "4.4.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "lts"
  },
  "scripts": {
    "start": "ts-node src/main.ts",
    "start:dev": "nodemon",
    "start:local": "ROUTE_PREFIX=/user ts-node src/main.dev.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json && cpy 'src/**/*.csv' './dist/src' && cpy 'views/**/*' './dist/views'",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "get-jwt-token": "DATABASE_URL=\"postgres://postgres:kidsloop@localhost/postgres\" ts-node \"scripts/getJwtToken.ts\"",
    "prettier-format": "prettier --config .prettierrc \"./**/*.{ts,test.ts}\" --write",
    "prettier-check": "prettier --config .prettierrc \"./**/*.{ts,test.ts}\" --check",
    "commitlint-check": "commitlint",
    "populate-database-extra-memory": "DATABASE_URL=\"postgres://postgres:kidsloop@localhost/postgres1\" node --max-old-space-size=8096 -- node_modules/.bin/ts-node -P tsconfig.json \"tests/populateDatabase.ts\"",
    "populate-database": "DATABASE_URL=\"postgres://postgres:kidsloop@localhost/postgres1\" ts-node \"tests/populateDatabase.ts\"",
    "update-role-permissions": "ts-node \"tests/updateSystemRolePermissions.ts\"",
    "update-permission-names": "ts-node \"tests/updatePermissionNames.ts\"",
    "test:unit": "NODE_ENV=development LOG_LEVEL=info mocha --require ts-node/register --file tests/unit/hooks.ts \"tests/unit/**/*.test.ts\" --timeout 8000",
    "test:integration": "NODE_ENV=development LOG_LEVEL=info mocha --require ts-node/register --file tests/integration/hooks.ts \"tests/integration/**/*.test.ts\" --timeout 8000",
    "test:acceptance": "DATABASE_URL=postgres://postgres:kidsloop@localhost/postgres1 LOG_LEVEL=info mocha --require ts-node/register --file tests/acceptance/hooks.ts \"tests/acceptance/**/*.test.ts\" --timeout 8000",
    "test:migrations": "NODE_ENV=development LOG_LEVEL=info mocha --require ts-node/register \"tests/migrations/**/*.test.ts\" --timeout 8000",
    "test": "npm run test:unit && npm run test:integration && npm run test:migrations",
    "coverage:unit": "nyc --all --require ts-node/register -n src --reporter=lcov --reporter=json-summary --reporter=text-summary --report-dir ./coverage_unit npm run test:unit",
    "coverage:migrations": "nyc --all --require ts-node/register -n migrations --reporter=lcov --reporter=json-summary --reporter=text-summary --report-dir ./coverage_migrations npm run test:migrations",
    "coverage:integration": "nyc --all --require ts-node/register -n src --reporter=lcov --reporter=json-summary --reporter=text-summary --report-dir ./coverage_integration npm run test:integration",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:KL-Engineering/user-service.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/KL-Engineering/user-service/blob/main/README.md",
